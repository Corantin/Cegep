<Window x:Class="Animation.StretchImage"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="StretchImage" Height="300" Width="300">
    <Grid>
        <Grid.ColumnDefinitions>
            <ColumnDefinition></ColumnDefinition>
            <ColumnDefinition></ColumnDefinition>
            <ColumnDefinition></ColumnDefinition>
        </Grid.ColumnDefinitions>
        <Grid.RowDefinitions>
            <RowDefinition></RowDefinition>
            <RowDefinition></RowDefinition>
        </Grid.RowDefinitions>
        <Image Source="chat.jpg" Grid.Column="0" Grid.Row="0" Stretch="Uniform"></Image>
        <Image Source="chat.jpg" Grid.Column="0" Grid.Row="1" Stretch="UniformToFill"></Image>
        <Image Source="chat.jpg" Grid.Column="1" Grid.Row="0" Stretch="Fill"></Image>
        <Image Source="chat.jpg" Grid.Column="1" Grid.Row="1" Stretch="None"></Image>
        <Image Source="chat.jpg" Grid.Column="2" Grid.Row="0"></Image>
        <Image Source="chat.jpg" Grid.Column="2" Grid.Row="1"></Image>
    </Grid>
</Window>
