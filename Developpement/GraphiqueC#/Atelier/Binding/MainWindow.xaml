<Window x:Class="Binding.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="MainWindow" Height="350" Width="525">
    <Canvas>
        <Label Canvas.Top="10" Canvas.Left="30">Rencontre</Label>
        <Label Canvas.Top="50" Canvas.Left="10">Sujet:</Label>
        <TextBox Name="RencontreSujet" Canvas.Top="50" Canvas.Left="60" Width="200" TextChanged="RencontreSujet_TextChanged"></TextBox>
        <Label Canvas.Top="80" Canvas.Left="10">Date:</Label>
        <DatePicker Name="RencontreDate" Canvas.Top="80" Canvas.Left="60" SelectedDateChanged="RencontreDate_SelectedDateChanged"></DatePicker>
        <Button Name="AfficherModele" Canvas.Top="120" Canvas.Left="70" Click="AfficherModele_Click">Afficher Modèle</Button>
        <Button Name="ModifierModele" Canvas.Top="150" Canvas.Left="70" Click="ModifierModele_Click">Modifier Modèle</Button>

    </Canvas>
</Window>
